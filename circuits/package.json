{
  "name": "zylith-circuits",
  "version": "0.1.0",
  "description": "Circom circuits for Zylith shielded CLMM",
  "scripts": {
    "compile:membership": "circom membership.circom --r1cs --wasm --sym -o build/",
    "compile:swap": "circom swap.circom --r1cs --wasm --sym -o build/",
    "compile:liquidity": "circom liquidity.circom --r1cs --wasm --sym -o build/",
    "compile:all": "bun run compile:membership && bun run compile:swap && bun run compile:liquidity",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "circom",
    "zk-snark",
    "privacy",
    "defi",
    "starknet"
  ],
  "author": "Zylith Protocol",
  "license": "MIT",
  "dependencies": {
    "circomlib": "^2.0.5",
    "snarkjs": "^0.7.0"
  },
  "devDependencies": {
    "circom": "^2.2.0"
  }
}