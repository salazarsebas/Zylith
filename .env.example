# Starknet Sepolia deployment configuration
# RPC: v0_8 required for starkli 0.4.x (TX v3 / STRK gas)
STARKNET_RPC_URL=https://starknet-sepolia.g.alchemy.com/starknet/version/rpc/v0_8/<YOUR_API_KEY>

# Account & keystore file paths (relative to project root)
STARKNET_ACCOUNT=scripts/starknet-account/account.json
STARKNET_KEYSTORE=scripts/starknet-account/keystore.json

# Keystore password (for non-interactive deploy)
STARKNET_KEYSTORE_PASSWORD=<YOUR_PASSWORD>

# Admin/deployer address (computed by starkli account oz init)
ADMIN_ADDRESS=<YOUR_ADMIN_ADDRESS>
